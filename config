Host *
	User ubuntu
	IdentityFile id_rsa
	StrictHostKeyChecking no
	PasswordAuthentication no

Host bastionNSO
	HostName 91.123.203.75
	ForwardAgent yes
	ControlMaster auto 
	ControlPath ~/.ssh/ansible-%r@%h:%p
	ControlPersist yes
	
Host HAproxy
	HostName 91.123.203.193
	ForwardAgent yes
	ControlMaster auto 
	ProxyCommand ssh -W %h:%p bastionNSO

Host devA
	HostName 10.0.1.12
	ProxyCommand ssh -W %h:%p bastionNSO

